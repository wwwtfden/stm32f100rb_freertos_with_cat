{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "cortex-debug",
      "request": "launch",
      "name": "Debug (OpenOCD)",
      "servertype": "openocd",
      "cwd": "${workspaceFolder}",
      "runToEntryPoint": "main",
      "executable": "./build/freertos_vl_test.elf",
      "device": "STM32F100RB",
      "configFiles": [
        "interface/stlink.cfg",
        "target/stm32f1x.cfg"
      ],
      "showDevDebugOutput": "none"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/wtfden/work/freertos_vl_test",
      "program": "/home/wtfden/work/freertos_vl_test/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}